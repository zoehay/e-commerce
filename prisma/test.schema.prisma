generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("TEST_DATABASE_URL")
}

model Product {
  id Int @id @default(autoincrement())
  name String
  description String
  price Decimal @db.Money
}

model User {
  id Int @id @default(autoincrement())
  email String @unique
  password String 
  name String?
}

model Session {
  id        String   @id
  sid       String   @unique
  data      String
  expiresAt   DateTime
}